<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:layout_marginTop="6dip" android:orientation="horizontal">
	<TextView android:id="@+id/data_array" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:text="d[x] = "
		android:textColor="@color/sensor_attributes" android:textSize="14dip"></TextView>
	<TextView android:id="@+id/data_label" android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:textColor="@color/sensor_attributes"
		android:text="label = " android:textSize="14dip"></TextView>
	<TextView android:id="@+id/data_value" android:layout_height="wrap_content"
		android:text="0.0" android:textStyle="bold" android:textColor="@color/white" android:textSize="18dip" android:layout_width="fill_parent"></TextView>
</LinearLayout>
